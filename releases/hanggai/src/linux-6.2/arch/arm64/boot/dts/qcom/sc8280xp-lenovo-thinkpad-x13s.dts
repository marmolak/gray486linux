	status = "okay";
};

&qup0_i2c4 {
	clock-frequency = <400000>;

	pinctrl-names = "default";
	pinctrl-0 = <&qup0_i2c4_default>;

	status = "okay";

	/* FIXME: verify */
	touchscreen@10 {
		compatible = "hid-over-i2c";
		reg = <0x10>;

		hid-descr-addr = <0x1>;
